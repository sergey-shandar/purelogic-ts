{
    "compilerOptions": {
        "declaration": true,
        "module": "commonjs",
        "target": "es2015",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "pretty": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noEmitHelpers": true
    },
    "exclude": [
        "node_modules",
        ".c9"
    ]
}
