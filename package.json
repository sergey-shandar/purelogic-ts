{
  "name": "purelogic-ts",
  "version": "0.0.0",
  "description": "PureLogic for TypeScript",
  "main": "index.js",
  "scripts": {
    "prepublish": "typings install && tsc",
    "test": "tsc && tslint *.ts && npm run istanbul-cover-check && npm run browserify-ui",
    "istanbul-cover-check": "istanbul cover node_modules/mocha/bin/_mocha && istanbul check-coverage",
    "browserify-ui": "browserify -r ./ui.js:ui -o bundle.js",
    "mocha": "mocha",
    "typings": "typings",
    "istanbul": "istanbul",
    "tslint": "tslint",
    "tsc": "tsc",
    "browserify": "browserify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergey-shandar/purelogic-ts.git"
  },
  "author": "Sergey Shandar",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sergey-shandar/purelogic-ts/issues"
  },
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "homepage": "https://github.com/sergey-shandar/purelogic-ts#readme",
  "devDependencies": {
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "tslint": "^3.11.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
